{
  "name": "burnham",
  "version": "1.0.0",
  "description": "Theme for the Oregon Zoo, based on Oregon Metro's Minima.",
  "repository": "https://github.com/ORMetro/zoo-galaxy",
  "author": "Oregon Metro + Forum One",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-transform-block-scoping": "^7.15.3",
    "@babel/plugin-transform-parameters": "^7.14.5",
