    "add-attributes-twig-extension": "^0.1.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-preset-minify": "^0.5.0",
    "bem-twig-extension": "^0.1.1",
    "bootstrap": "^5.0.1",
    "chromatic": "^6.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "cypress-real-events": "^1.7.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "fs": "^0.0.1-security",
    "glob": "^9.3.0",
    "jquery": "^3.6.3",
    "js-yaml-loader": "^1.2.2",
    "mini-css-extract-plugin": "^2.7.3",
    "nani": "^3.2.1",
    "node-sass-glob-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "npm-watch": "^0.11.0",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "postcss-custom-properties": "^13.1.4",
    "postcss-loader": "^7.1.0",
    "sass": "^1.49.0",
    "sass-loader": "^13.2.0",
    "storybook": "6.5",
    "style-loader": "^3.3.2",
    "svg-sprite-loader": "^6.0.11",
    "twig-drupal-filters": "^3.1.0",
    "twig-loader": "https://github.com/fourkitchens/twig-loader",
    "wait-on": "^7.0.1",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.1.4"
  },
  "watch": {
    "build-config": "components/00-base/config.design-tokens.yml"
  },
  "scripts": {
    "build": "npm run build-config && webpack --config ./webpack/webpack.prod.js",
    "build-config": "node lib/buildTokens.js",
    "build-storybook": "build-storybook -s ./dist,./images:/,./fonts -o .out",
    "chromatic": "chromatic --exit-zero-on-changes",
    "commit": "git-cz",
    "coverage": "yarn test && open-cli .coverage/lcov-report/index.html",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run",
    "develop": "npm run watch & concurrently --raw \"npm run webpack\" \"npm run storybook\"",
    "format": "prettier --write \"**/*.{js,yml,scss,md}\"",
    "lint": "eslint ./components && stylelint \"./components/**/*.scss\"",
    "lint-staged": "lint-staged",
    "prepare": "cd ../../../.. && husky install docroot/themes/custom/burnham/.husky",
    "storybook": "start-storybook --ci -s ./dist,./images:/,./fonts -p 6006",
    "test-a11y": "npm run build-storybook && npx sb extract .out .out/stories.json && ./scripts/a11y.js -r",
    "test-jest": "jest --coverage",
    "test-cypress": "npm run storybook & npx wait-on http://localhost:6006 && npm run cypress:run && ps -ef | grep storybook | grep -v grep | awk '{print $2}' | xargs kill",
    "twatch": "jest --no-coverage --watch --verbose",
    "webpack": "webpack --watch --config ./webpack/webpack.dev.js",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5",
    "@storybook/addon-actions": "^6.5",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.5",
    "@storybook/addon-links": "^6.5",
    "@storybook/addons": "^6.5.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/html": "^6.5",
    "@storybook/manager-webpack5": "^6.5.16",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "babel-jest": "^29.5.0",
    "chalk": "^5.2.0",
    "commitizen": "^4.1.2",
    "core-js": "^3.19.2",
    "cypress": "^12.8.1",
    "cypress-plugin-tab": "^1.0.5",
    "cz-conventional-changelog": "3.3.0",
    "esbuild-loader": "^3.0.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.0",
    "husky": "^8.0.3",
    "image-minimizer-webpack-plugin": "^3.8.2",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "open-cli": "^7.1.0",
    "pa11y": "^6",
    "prettier": "^2.8.4",
    "ramda": "^0.28.0",
    "sharp": "^0.31.3",
    "storybook-addon-pseudo-states": "^1.15.2",
    "stylelint": "^15.2.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-webpack-plugin": "^4.1.0",
    "svgo": "^3.0.2",
    "twig": "^1.15.4",
    "twig-testing-library": "^1.1.1",
    "typescript": "^4.1.3",
    "yaml": "^1.10.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
